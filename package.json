{
  "name": "orfeo",
  "displayName": "Orfeo",
  "description": "Orfeo and Musicline language support for TextMate based systems.",
  "version": "0.1.0",
  "publisher": "Orfeo",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "musicline",
        "extensions": [".musicline"]
      },
      {
        "id": "orfeo",
        "extensions": [".orfeo"],
        "configuration": "./vscode/orfeo.language-configuration.json"
      },
      {
        "id": "orfeoError",
        "extensions": [".orfeoerror"]
      }
    ],
    "grammars": [
      {
        "language": "musicline",
        "scopeName": "source.musicline",
        "path": "./syntaxes/musicline.tmLanguage.json"
      },
      {
        "language": "orfeo",
        "scopeName": "source.orfeo",
        "path": "./syntaxes/orfeo.tmLanguage.json"
      },
      {
        "language": "orfeoError",
        "scopeName": "source.orfeoerror",
        "path": "./syntaxes/orfeoerror.tmLanguage.json"
      }
    ]
  }
}
